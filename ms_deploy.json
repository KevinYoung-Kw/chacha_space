{
  "$schema": "https://modelscope.cn/api/v1/studios/deploy_schema.json",
  "sdk_type": "docker",
  "resource_configuration": "platform/2v-cpu-16g-mem",
  "port": 7860,
  "environment_variables": [
    {
      "name": "NODE_ENV",
      "value": "production"
    },
    {
      "name": "PORT",
      "value": "7860"
    },
    {
      "name": "DATABASE_PATH",
      "value": "/app/data/chacha.db"
    },
    {
      "name": "STATIC_PATH",
      "value": "/app/backend/frontend/dist"
    },
    {
      "name": "CORS_ORIGIN",
      "value": "*"
    },
    {
      "name": "JWT_SECRET",
      "value": "chacha-modelscope-secret-key-2024"
    }
  ]
}
